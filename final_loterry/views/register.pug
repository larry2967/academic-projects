<!DOCTYPE html>
<html>
  <head>
    <title>Register Page</title>
    <link rel='stylesheet' href='/stylesheets/register.css'>
  </head>
  <body>
    <h1>
        |Register Page
    </h1>
    <form action="" method="post">
        <input type="text" id="id" name="user_id">
        <input type="text" id="pass" name="user_password">
        <input type="text" id="account" name="user_account">
        <input type="submit">
    </form>
    <p>
        |#{router_method}
    </p>
    |#{id}
    |#{pass}
    |#{account}
    |#{rows}

    <script src="/javascripts/register.js" type="text/javascript"></script>
  </body>
</html>
